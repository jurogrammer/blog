    <script>
        var prefix = '';
        var segments = window.location.pathname.split('/');
        if (segments.length >= 2) {
            if (segments[1].length === 2) {
                prefix = segments[1];
            }
        }
        prefix = prefix ? prefix + '/' : '';
        var baseUrl = window.location.protocol + "//" + window.location.host + "/" + prefix;

        var normalRequest = function () {
            $.ajax({
                url: baseUrl + 'ajax/user',
                crossDomain: true,
                dataType: 'json',
                xhrFields: {
                    withCredentials: true
                }
            }).done(successFunc);
        };
        var successFunc = function(data){
            if (data.user && data._token) {
                if (data.user && data._token) {
                    console.log('Logged-in user session detected.');
                    const href = $('.navigation-menu .nav-user a').attr('href');
                    $('.navigation-menu .nav-user a').attr('href', href.replace(/\?.*/, ''));
                    $('.navigation-menu .nav-user a .caption').text(data.translations['my_account']);
                    $('.navigation-menu .nav-user .sign-in-icon').hide();
                    $('.navigation-menu .nav-user .account-icon').show();

                    const signOutIcon = $('.navigation-menu .nav-user .sign-out-icon').html();
                    if (!$('.navigation-menu .nav-logout').length) {
                        $('.navigation-menu .nav-user').after('<li class="nav-logout"><form method="post" action="'+ window.localized_url_prefix_m + 'logout"><input name="_token" type="hidden" value="' + data._token + '" ><button title="' + data.translations['sign_out'] + '">' + signOutIcon + ' <span class="caption d-none d-xl-inline-block">' + data.translations['sign_out'] + '</span></button></form></li>');
                    }
                }
            }
        };
        normalRequest();
